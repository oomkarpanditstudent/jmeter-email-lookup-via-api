            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.text.SimpleDateFormat;

registrationEmailId=vars.get(&quot;email&quot;);
registerationDate1 = vars.get(&quot;dateRegistered&quot;);
registerationTime1 = vars.get(&quot;timeRegistered&quot;);
emailReceivedDate= vars.get(&quot;dateReceived&quot;);
emailReceivedTime= vars.get(&quot;timeReceived&quot;);

SimpleDateFormat sdf = new SimpleDateFormat(&quot;HH:mm:ss&quot;);

       long  regTime =    sdf.parse(registerationTime1).getTime();
       long  postedTime = sdf.parse(emailReceivedTime).getTime();
       long  res = (postedTime - regTime)/1000;
log.info(String.parse(res));

f = new FileOutputStream(&quot;C:\\BitBucket\\RoyalMail_WDIO\\webdriverio-cucumber\\result.csv&quot;, true);
p = new PrintStream(f);
this.interpreter.setOut(p);
print(registrationEmailId+&quot;,&quot;+registerationDate1+&quot;,&quot;+emailReceivedDate+&quot;,&quot;+registerationTime1+&quot;,&quot;+emailReceivedTime+&quot;,&quot;+res/1000);

f.close();
</stringProp>
            <stringProp name="scriptLanguage">beanshell</stringProp>
          </JSR223PostProcessor>
