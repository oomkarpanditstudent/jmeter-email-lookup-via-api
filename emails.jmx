          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.text.SimpleDateFormat;

registrationEmailId=vars.get(&quot;email&quot;);
registerationDate1 = vars.get(&quot;dateRegistered&quot;);
registerationTime1 = vars.get(&quot;timeRegistered&quot;);
emailReceivedDate= vars.get(&quot;dateReceived&quot;);
emailReceivedTime= vars.get(&quot;timeReceived&quot;);

SimpleDateFormat sdf = new SimpleDateFormat(&quot;HH:mm:ss.SSS&quot;);

       long  regTime =    sdf.parse(registerationTime1).getTime();
       long  postedTime = sdf.parse(emailReceivedTime).getTime();
       long  res = (postedTime - regTime);
       int result= Math.round(res/1000.0);

f = new FileOutputStream(&quot;result.csv&quot;, true);
p = new PrintStream(f);
this.interpreter.setOut(p);
print(registrationEmailId+&quot;,&quot;+registerationDate1+&quot;,&quot;+emailReceivedDate+&quot;,&quot;+registerationTime1+&quot;,&quot;+emailReceivedTime+&quot;,&quot;+result);

f.close();
</stringProp>
          </BeanShellPostProcessor>
